# THIS IS A SEQUENZ TASK CLASS DEFINITION FILE
# See full_template.def for documented entries

%NAME
    typical_task

%OWNER
    bob 

%VALID_HOURS
    0,6,12,18

%EXTERNAL_TASK
    run-typical_task.sh

%PREREQUISITES
    file input_$(MY_REFERENCE_TIME)_utc.nc ready

%POSTREQUISITES
            0 min: typical_task started for $(MY_REFERENCE_TIME)
    6,18 | 18 min: file typical_task-foo_$(MY_REFERENCE_TIME).nc ready
    0,12 | 18 min: file typical_task-bar_$(MY_REFERENCE_TIME).nc ready
           20 min: typical_task finished for $(MY_REFERENCE_TIME)
