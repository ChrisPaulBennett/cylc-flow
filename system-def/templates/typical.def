# THIS IS A SEQUENZ TASK CLASS DEFINITION FILE
# See full_template.def for documented entries

%NAME
    typical_task

%TYPE
    sequential

%OWNER
    bob 

%VALID_HOURS
    0,6,12,18

%EXTERNAL_TASK
    run-typical_task.sh

%PREREQUISITES
    file met_$(MY_REFERENCE_TIME)_utc.nc ready

%RUN_LENGTH
    20 

%OUTPUTS
    6,18 | 18: file typical_task-foo_$(MY_REFERENCE_TIME).nc ready
    0,12 | 18: file typical_task-bar_$(MY_REFERENCE_TIME).nc ready
