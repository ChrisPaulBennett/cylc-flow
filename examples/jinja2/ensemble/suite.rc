#!jinja2

title = Jinja2 simple ensemble example
description = """
Illustrates programmatic generation of dependencies for related tasks.
The same can be done for runtime settings."""

{% set N_MEMBERS = 5 %}
[scheduling]
    [[dependencies]]
        graph = """ {# generate ensemble dependencies #}
        {% for I in range( 0, N_MEMBERS ) %}
          foo => mem_{{ I }} => post_{{ I }} => bar
        {% endfor %}"""
