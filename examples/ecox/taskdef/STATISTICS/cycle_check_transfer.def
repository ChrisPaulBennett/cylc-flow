# THIS IS A CYLC TASK CLASS DEFINITION FILE

%TASK
    cycle_check_transfer, c_c_t

%DESCRIPTION
    Trasnfer statistics file to web server.

%TYPE
    free, sequential

%OWNER
    ecoconnect

%HOURS
    0,6,12,18

%ENVIRONMENT
	SRCE ${HOME}/output/stats/task_stats_${CYCLE_TIME}.csv ${HOME}/output/stats/task_stats_${CYCLE_TIME}.csv
	DEST ecoconnect_oper@pa:/oper/ecoconnect_oper/public_html/eco_check/stats/task_stats_latest.csv ecoconnect_oper@pa:/oper/ecoconnect_oper/public_html/eco_check/stats/task_stats_${CYCLE_TIME}.csv
	RECOPY true true

%COMMAND
    cylc-wrapper _file-transfer
                                
%PREREQUISITES
    "cycle_check%$(CYCLE_TIME) finished"

%OUTPUTS
    # Default
