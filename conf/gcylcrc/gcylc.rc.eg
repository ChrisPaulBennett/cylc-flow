
# To customize your gcylc task state color palettes copy this file to
# $HOME/.cylc/gcylc.rc and adapt to your needs. Then to see what themes 
# are available: 'gcylc -l|--list-themes'.

use theme = my_theme

[themes]
    [[my_theme]] # override the 'running' color in the 'classic' theme
        inherit = classic
        running = "color=#ff00ff" # magenta

