title = Generate log messages for various suite connection types.
[scheduling]
    [[dependencies]]
        graph = foo
[runtime]
    [[foo]]
        script = """
sleep 5
# COMMAND INTERFACE:
cylc hold $CYLC_SUITE_NAME
# INFO INTERFACE:
cylc show $CYLC_SUITE_NAME
# BROADCAST INTERFACE:
cylc broadcast -d $CYLC_SUITE_NAME
# COMMAND INTERFACE:
cylc release $CYLC_SUITE_NAME
sleep 5"""
