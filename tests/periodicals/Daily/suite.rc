[cylc]
   [[reference test]]
       required run mode = live
       live mode suite timeout = 1.0 # minutes
[scheduling]
    initial cycle time = 2010010100
    final cycle time = 2010010500
    runahead limit = 24 # enforce sequential running
    [[dependencies]]
        [[[Daily(20100101,2)]]]
            graph = "a"
[runtime]
    [[a]]
        command scripting = "sleep 1"
