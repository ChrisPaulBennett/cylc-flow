%TASK_NAME
    global_prep

%VALID_REFERENCE_TIMES
    0

%EXTERNAL_TASK
    global_prep.sh

%USER_PREFIX
    wave

%PREREQUISITES
    file 10mwind_$(REFERENCE_TIME).um ready
    file seaice_$(REFERENCE_TIME).um ready

%POSTREQUISITES
    0 min: $(TASK_NAME) started for $(REFERENCE_TIME)
    5 min: file 10mwind_$(REFERENCE_TIME).nc ready
    7 min: file seaice_$(REFERENCE_TIME).nc ready
    10 min: $(TASK_NAME) finished for $(REFERENCE_TIME)
