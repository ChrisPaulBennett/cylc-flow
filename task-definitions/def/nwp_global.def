
# THIS IS A SEQUENZ TASK CLASS DEFINITION FILE
#   (see template.def for more information)

%TASK_NAME
    nwp_global

%VALID_REFERENCE_TIMES
    0

%EXTERNAL_TASK
    nwp_global.sh

%USER_PREFIX
    nwp

%PREREQUISITES
    file 10mwind_$(CURRENT_REFERENCE_TIME).um ready

%POSTREQUISITES
    0 min: $(TASK_NAME) started for $(CURRENT_REFERENCE_TIME)
    120 min: file 10mwind_$(CURRENT_REFERENCE_TIME).nc ready
    121 min: $(TASK_NAME) finished for $(CURRENT_REFERENCE_TIME)
