# THIS IS A cylon TASK CLASS DEFINITION FILE
# See full_template.def for documented entries

%NAME
    startup

%DESCRIPTION
    Clean out the system working directory at startup.
    Provide initial restart inputs for the forecast models at startup. 

%TYPE
    free_task, oneoff

%VALID_HOURS
    0,6,12,18

%EXTERNAL_TASK
    task-wrapper

%EXPORT
    WRAP startup.sh

%PREREQUISITES
    # None

%RUN_LENGTH_MINUTES
    10

%OUTPUTS
    7: A restart files ready for $(MY_REFERENCE_TIME)
    8: B restart files ready for $(MY_REFERENCE_TIME)
    9: C restart files ready for $(MY_REFERENCE_TIME)

%COTEMPORAL_DEPENDANTS_ONLY
    True
