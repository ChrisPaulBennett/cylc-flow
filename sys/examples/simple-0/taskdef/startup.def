# THIS IS A CYLC TASK DEFINITION FILE

%NAME
    startup

%DESCRIPTION
    Clean out the system working directory at startup.
    Provide initial restart inputs for the forecast models at startup. 

%TYPE
    free_task, oneoff

%START_TIMES
    0,6,12,18

%EXTERNAL_TASK
    cylc-task-wrapper

%ENVIRONMENT
    WRAP startup.sh

%PREREQUISITES
    # None

%ESTIMATED_RUN_TIME
    10

%OUTPUTS
    7: "A restart files ready for $(MY_CYCLE_TIME)"
    8: "B restart files ready for $(MY_CYCLE_TIME)"
    9: "C restart files ready for $(MY_CYCLE_TIME)"

%SINGLE_CYCLE
    True
