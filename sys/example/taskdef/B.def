# THIS IS A cyclon TASK CLASS DEFINITION FILE
# See full_template.def for documented entries

%NAME
    B

%TYPE
    normal

%VALID_HOURS
    0,6,12,18

%EXTERNAL_TASK
    task-wrapper

%EXPORT
    WRAP B.sh

%PREREQUISITES
    A%$(MY_REFERENCE_TIME) finished
    B restart files ready for $(MY_REFERENCE_TIME)

%STARTUP_PREREQUISITES
    A%$(MY_REFERENCE_TIME) finished

%RUN_LENGTH_MINUTES
    60

%OUTPUTS
    20: B restart files ready for $(MY_REFERENCE_TIME +  6 )
    40: B restart files ready for $(MY_REFERENCE_TIME + 12 )
    59: B restart files ready for $(MY_REFERENCE_TIME + 18 )
