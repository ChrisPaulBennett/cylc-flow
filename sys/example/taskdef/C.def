# THIS IS A cyclon TASK CLASS DEFINITION FILE
# See full_template.def for documented entries

%NAME
    C

%TYPE
    normal

%VALID_HOURS
    0,6,12,18

%EXTERNAL_TASK
    task-wrapper

%EXPORT
    WRAP C.sh

%PREREQUISITES
    A%$(MY_REFERENCE_TIME) finished
    C restart files ready for $(MY_REFERENCE_TIME)

%STARTUP_PREREQUISITES
    A%$(MY_REFERENCE_TIME) finished

%RUN_LENGTH_MINUTES
    120

%OUTPUTS
    20: C restart files ready for $(MY_REFERENCE_TIME +  6 )
    40: C restart files ready for $(MY_REFERENCE_TIME + 12 )
    59: C restart files ready for $(MY_REFERENCE_TIME + 18 )
