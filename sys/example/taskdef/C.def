# THIS IS A cyclon TASK CLASS DEFINITION FILE
# See full_template.def for documented entries

%NAME
    C

%DESCRIPTION
    Example system task C, represents a forecast model (i.e. it depends
    on restart files generated by its previous instance).
    Also depends on task A.

%TYPE
    normal

%VALID_HOURS
    0,6,12,18

%EXTERNAL_TASK
    task-wrapper

%EXPORT
    WRAP C.sh
    NEXT_REFERENCE_TIME $(MY_REFERENCE_TIME + 6)
    NEXT_NEXT_REFERENCE_TIME $(MY_REFERENCE_TIME + 12)

%PREREQUISITES
    A%$(MY_REFERENCE_TIME) finished
    C restart files ready for $(MY_REFERENCE_TIME)

%RUN_LENGTH_MINUTES
    120

%OUTPUTS
    20: C restart files ready for $(MY_REFERENCE_TIME +  6 )
    40: C restart files ready for $(MY_REFERENCE_TIME + 12 )
#    59: C restart files ready for $(MY_REFERENCE_TIME + 18 )
